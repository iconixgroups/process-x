{"ast":null,"code":"import axios from 'axios';\nimport { BASE_URL } from '../../api/base.js';\nexport default {\n  name: 'ModulePublication',\n  data() {\n    return {\n      loading: true,\n      error: null,\n      module: null\n    };\n  },\n  methods: {\n    async fetchModule() {\n      try {\n        const moduleId = this.$route.params.moduleId;\n        const response = await axios.get(`${BASE_URL}/settings/module/${moduleId}`);\n        this.module = response.data;\n        this.loading = false;\n      } catch (error) {\n        this.error = \"Failed to load module data.\";\n        this.loading = false;\n      }\n    },\n    async publishModule() {\n      try {\n        const moduleId = this.module.moduleId;\n        await axios.post(`${BASE_URL}/publish/module/${moduleId}`, {\n          publish: true\n        });\n        this.module.isPublished = true;\n        alert('Module published successfully!');\n      } catch (error) {\n        this.error = \"Failed to publish module.\";\n      }\n    }\n  },\n  created() {\n    this.fetchModule();\n  }\n};","map":{"version":3,"names":["axios","BASE_URL","name","data","loading","error","module","methods","fetchModule","moduleId","$route","params","response","get","publishModule","post","publish","isPublished","alert","created"],"sources":["src/components/ModulePublication.vue"],"sourcesContent":["<template>\n  <div class=\"module-publication\">\n    <h1>Publish Module</h1>\n    <div v-if=\"loading\" class=\"loading\">Loading...</div>\n    <div v-else>\n      <div v-if=\"error\" class=\"error\">{{ error }}</div>\n      <div v-if=\"module\">\n        <h2>{{ module.moduleName }}</h2>\n        <p>Module ID: {{ module.moduleId }}</p>\n        <button @click=\"publishModule\" :disabled=\"module.isPublished\">Publish</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport { BASE_URL } from '../../api/base.js';\nexport default {\n  name: 'ModulePublication',\n  data() {\n    return {\n      loading: true,\n      error: null,\n      module: null\n    };\n  },\n  methods: {\n    async fetchModule() {\n      try {\n        const moduleId = this.$route.params.moduleId;\n        const response = await axios.get(`${BASE_URL}/settings/module/${moduleId}`);\n        this.module = response.data;\n        this.loading = false;\n      } catch (error) {\n        this.error = \"Failed to load module data.\";\n        this.loading = false;\n      }\n    },\n    async publishModule() {\n      try {\n        const moduleId = this.module.moduleId;\n        await axios.post(`${BASE_URL}/publish/module/${moduleId}`, { publish: true });\n        this.module.isPublished = true;\n        alert('Module published successfully!');\n      } catch (error) {\n        this.error = \"Failed to publish module.\";\n      }\n    }\n  },\n  created() {\n    this.fetchModule();\n  }\n};\n</script>\n\n<style scoped>\n.module-publication {\n  padding: 20px;\n}\n\n.loading {\n  color: #888;\n}\n\n.error {\n  color: red;\n}\n\nbutton {\n  margin-top: 10px;\n  padding: 10px 20px;\n  background-color: #5cb85c;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\nbutton:disabled {\n  background-color: #ccc;\n  cursor: not-allowed;\n}\n</style>"],"mappings":"AAgBA,OAAAA,KAAA;AACA,SAAAC,QAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,OAAA;MACAC,KAAA;MACAC,MAAA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,YAAA;MACA;QACA,MAAAC,QAAA,QAAAC,MAAA,CAAAC,MAAA,CAAAF,QAAA;QACA,MAAAG,QAAA,SAAAZ,KAAA,CAAAa,GAAA,IAAAZ,QAAA,oBAAAQ,QAAA;QACA,KAAAH,MAAA,GAAAM,QAAA,CAAAT,IAAA;QACA,KAAAC,OAAA;MACA,SAAAC,KAAA;QACA,KAAAA,KAAA;QACA,KAAAD,OAAA;MACA;IACA;IACA,MAAAU,cAAA;MACA;QACA,MAAAL,QAAA,QAAAH,MAAA,CAAAG,QAAA;QACA,MAAAT,KAAA,CAAAe,IAAA,IAAAd,QAAA,mBAAAQ,QAAA;UAAAO,OAAA;QAAA;QACA,KAAAV,MAAA,CAAAW,WAAA;QACAC,KAAA;MACA,SAAAb,KAAA;QACA,KAAAA,KAAA;MACA;IACA;EACA;EACAc,QAAA;IACA,KAAAX,WAAA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}